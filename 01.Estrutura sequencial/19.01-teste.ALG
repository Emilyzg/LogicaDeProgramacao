Algoritmo "semnome"
// Disciplina  :  [Linguagem e Lógica de Programação] 
// Professor   : Antonio Carlos Nicolodi 

Var
a : inteiro
b : real
c : caractere
d : logico

Inicio

a <- 34
b <- 2.999
c <- "Ola!"
d <- verdadeiro

escreval(a)
escreval(b)
escreval (b:10)
escreval(b:10:2)
escreval(c)
escreval(d)

Fimalgoritmo